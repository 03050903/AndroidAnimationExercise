<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_utril_pull_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="home.smart.fly.animationdemo.customview.activitys.UtrilPullViewActivity">

    <home.smart.fly.animationdemo.customview.views.pulltorefreshview.UtrilPulltoRefreshView
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:background="@color/background"
        android:layout_height="match_parent">

        <ListView
            android:id="@+id/listView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scrollbars="none" />

    </home.smart.fly.animationdemo.customview.views.pulltorefreshview.UtrilPulltoRefreshView>

</LinearLayout>
